<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.quicklistv_01">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".Class.Global"
        android:allowBackup="true"
        android:icon="@mipmap/logo2"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Home"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".SegundaPantallas.Acerca"
            android:label="Acerca de"
            android:parentActivityName=".Home"
            android:screenOrientation="portrait">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Home" />
        </activity>
        <activity
            android:name=".Login"
            android:screenOrientation="portrait" />
        <activity
            android:name=".SigIn"
            android:label="Registrarte"
            android:screenOrientation="portrait" />
        <activity
            android:name=".CursosDetail"
            android:label="Detalles"
            android:parentActivityName=".Home"
            android:screenOrientation="portrait">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Home" />
        </activity>
        <activity
            android:name=".SegundaPantallas.CalendarioCursos"
            android:label="Calendario"
            android:parentActivityName=".Home"
            android:screenOrientation="portrait">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Home" />
        </activity>
        <activity
            android:name=".SegundaPantallas.Alumnos"
            android:label="Alumnos"
            android:parentActivityName=".Home"
            android:screenOrientation="portrait">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Home" />
        </activity>
        <activity
            android:name=".SegundaPantallas.TomaAsistencia"
            android:label="Asistencia"
            android:parentActivityName=".SegundaPantallas.Alumnos"
            android:screenOrientation="portrait">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".SegundaPantallas.Alumnos" />
        </activity>
        <activity
            android:name=".SegundaPantallas.Preferences"
            android:label="Configuraciones"
            android:parentActivityName=".Home"
            android:screenOrientation="portrait">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Home" />
            >
        </activity>
        <activity
            android:name=".Splash"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Modificar"
            android:label="Modificar"
            android:parentActivityName=".Home"
            android:screenOrientation="portrait">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Home" />
        </activity>
        <activity
            android:name=".SegundaPantallas.VerSubGrupos"
            android:label="Subgrupos"
            android:parentActivityName=".Home"
            android:screenOrientation="portrait">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Home" />
        </activity>
        <activity
            android:name=".SegundaPantallas.CursosConsulta"
            android:label="Cursos"
            android:parentActivityName=".SegundaPantallas.CalendarioCursos"
            android:screenOrientation="portrait">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".SegundaPantallas.CalendarioCursos" />
            >
        </activity>
        <activity
            android:name=".SegundaPantallas.Notificaciones"
            android:label="Alerta de alumnos"
            android:parentActivityName=".SegundaPantallas.Alumnos"
            android:screenOrientation="portrait">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".SegundaPantallas.Alumnos" />

        </activity>
        <activity android:name=".SegundaPantallas.AlumnosDetail"
            android:label="Datos del alumno"
            android:parentActivityName=".SegundaPantallas.Notificaciones"
            android:screenOrientation="portrait">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".SegundaPantallas.Notificaciones" />>

        </activity>
    </application>

</manifest>